pipeline {
    agent any
    stages {
        stage("build") {
            steps {
                echo 'building an application'
                echo 'Application built is done'
            }
        }
        stage("test") {
            steps {
                echo 'testing an application'
            }
        }
        stage("deploy") {
            steps {
                echo 'deploying an application'
            }
        }
    }
}
